{
  "kind": "collectionType",
  "collectionName": "propiedades",
  "info": {
    "name": "Propiedades"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "habitaciones": {
      "type": "integer",
      "required": true
    },
    "estacionamiento": {
      "type": "integer",
      "required": true
    },
    "wc": {
      "type": "integer",
      "required": true
    },
    "descripcion": {
      "type": "richtext",
      "required": true
    },
    "precio": {
      "type": "biginteger",
      "required": true
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "categoria": {
      "model": "categorias"
    },
    "agentes": {
      "model": "agentes"
    }
  }
}
